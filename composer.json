{
    "name": "oneko/codeception-testlink",
    "description": "Codeception Extension for Testlink Reporting",
    "homepage": "https://github.com/oneko/codeception-testlink",
    "license": "MIT",
    "require": {
        "codeception/codeception": "^2.3",
        "kissifrot/php-ixr": "^1.8"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "OnekO\\Codeception\\TestLink\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OnekO\\Codeception\\TestLink\\Tests\\Unit\\": "tests/unit"
        }
    }
}
